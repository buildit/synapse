{
  "name": "synapse",
  "version": "1.0.0",
  "description": "Management information dashboard.",
  "main": "dev-server.js",
  "scripts": {
    "build": "gulp --environment development",
    "build:production": "gulp --environment production",
    "lint": "eslint src/ test/ *.js",
    "start": "gulp server",
    "test": "jest --verbose",
    "test:changed": "jest --verbose -o",
    "test:mocha": "mocha",
    "test-it-the-other-way": "node standalone-test.js"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/digitalrigbitbucketteam/synapse.git"
  },
  "author": "Buildit",
  "license": "MIT",
  "homepage": "https://bitbucket.org/digitalrigbitbucketteam/synapse#readme",
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json",
      "node"
    ],
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testRegex": "jest-tests/.*",
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils",
      "fbjs",
      "enzyme"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-jest": "^13.0.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "cowsay": "^1.1.8",
    "del": "^2.2.1",
    "enzyme": "^2.3.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "gulp": "^3.9.1",
    "gulp-connect": "^4.1.0",
    "gulp-html5-lint": "^1.1.0",
    "gulp-less": "^3.1.0",
    "gulp-lesshint": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "jest-cli": "^13.0.0",
    "react-addons-test-utils": "^15.1.0",
    "sinon": "^1.17.4",
    "vinyl-source-stream": "^1.1.0",
    "wdio-mocha-framework": "^0.3.5",
    "webdriverio": "^4.1.1",
    "yargs": "^4.8.1"
  },
  "dependencies": {
    "babel-preset-stage-2": "^6.11.0",
    "babelify": "^7.3.0",
    "bootstrap": "^3.3.6",
    "browserify": "^13.0.1",
    "es6-promise": "^3.2.1",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.0.0",
    "lodash": "^4.13.1",
    "moment": "^2.13.0",
    "react": "^15.1.0",
    "react-bootstrap": "^0.29.5",
    "react-dom": "^15.1.0",
    "react-highcharts": "^9.0.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.0",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  }
}
