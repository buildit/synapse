{
  "name": "synapse",
  "version": "1.0.1",
  "description": "Management information dashboard.",
  "main": "dev-server.js",
  "scripts": {
    "build": "echo 'Checking for outdated dependencies...' && npm outdated && gulp",
    "lint": "eslint src/ test/ *.js",
    "start": "gulp server",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o storybook",
    "test": "mocha --recursive --compilers js:babel-core/register --require babel-polyfill test/unit",
    "test:ci": "mocha --recursive --compilers js:babel-core/register --require babel-polyfill --reporter mocha-jenkins-reporter -O junit_report_path=./reports/test-results.xml,junit_report_name=\"Tests Results\",junit_report_stack=1 test/unit",
    "test:sagas": "mocha --recursive  --compilers js:babel-core/register --require babel-polyfill test/unit/middleware",
    "test:acceptance": "mocha --timeout 30000 --require co-mocha --require babel-register --require babel-polyfill --recursive test/acceptance/specs",
    "test:acceptance:ci": "mocha --timeout 60000 --require co-mocha --require babel-register --require babel-polyfill --recursive --reporter mocha-jenkins-reporter -O junit_report_path=./reports/acceptance-test-results.xml,junit_report_name=\"Acceptance Tests\",junit_report_stack=1 test/acceptance/specs",
    "validate": "npm ls --production",
    "version": "git add package.json",
    "postversion": "git push && git push origin HEAD:master && git push --tags",
    "generate-test-data": "node testApi/scripts/generateTestData"
  },
  "pre-commit#master": [
    "lint",
    "test",
    "test:acceptance",
    "validate"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/digitalrigbitbucketteam/synapse.git"
  },
  "author": "Buildit",
  "license": "MIT",
  "homepage": "https://bitbucket.org/digitalrigbitbucketteam/synapse#readme",
  "devDependencies": {
    "@kadira/storybook": "^2.15.1",
    "@kadira/storybook-addon-notes": "^1.0.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.14.0",
    "babel-root-slash-import": "^1.1.0",
    "chai": "^3.5.0",
    "chromedriver": "^2.23.1",
    "co-mocha": "^1.1.3",
    "connect-history-api-fallback": "^1.3.0",
    "cowsay": "^1.1.8",
    "css-loader": "^0.25.0",
    "del": "^2.2.1",
    "enzyme": "^2.3.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-connect": "^4.1.0",
    "gulp-html5-lint": "^1.1.0",
    "gulp-less": "^3.1.0",
    "gulp-lesshint": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-template": "^4.0.0",
    "gulp-util": "^3.0.7",
    "gulp-webpack": "^1.5.0",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "less-loader": "^2.2.3",
    "mocha": "^3.0.2",
    "mocha-jenkins-reporter": "^0.2.6",
    "path": "^0.12.7",
    "precommit-hook": "^3.0.0",
    "react-addons-test-utils": "^15.1.0",
    "selenium-webdriver": "^3.0.0-beta-2",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^1.16.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "babel-preset-stage-2": "^6.11.0",
    "bootstrap": "^3.3.6",
    "config-browserify": "^1.0.5",
    "connect-history-api-fallback": "^1.3.0",
    "d3": "^4.1.1",
    "es6-promise": "^3.2.1",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.0.0",
    "lodash": "^4.13.1",
    "moment": "^2.13.0",
    "react": "^15.1.0",
    "react-bootstrap": "^0.29.5",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "redux": "^3.5.2",
    "redux-saga": "^0.11.1",
    "redux-thunk": "^2.1.0",
    "sum-values-in-object": "^1.0.5",
    "xhr": "^2.2.2"
  }
}
